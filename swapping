#include<stdio.h>
int main()
{
int first,second,temp;
clrscr();
printf(“Enter first number: \n”);
scanf(“%d”,&first);
printf(“Enter second number: \n”);
scanf(“%d”,&second);
temp=first;
first=second;
second=temp;
printf(“After Swapping First Number =%d \n” ,first);
printf(“After Swapping Second Number =%d \n”, second);
getch();
return 0;
}
